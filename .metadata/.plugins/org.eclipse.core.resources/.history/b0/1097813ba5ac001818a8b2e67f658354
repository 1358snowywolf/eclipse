#include <iostream>
#include <fstream>
#include <unordered_map>

using namespace std;

int main() {
	ifstream fin ("milk.in");
	ofstream fout ("milk.out");
	int milk_needed = 0;
	int milk_suppliers = 0;
	unordered_map<int, pair<int, int>> used;

	fin >> milk_needed;
	fin >> milk_suppliers;

	for(int i = 0; i < milk_suppliers; i++){
		int price;
		int units;
		fin >> price;
		fin >> units;
		pair<int, int> input (units, 0);

		used.insert(price, input);
	}

	return 0;
}
